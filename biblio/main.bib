	note = {Accepted: 2018-02-22T18:48:44Z},
	file = {Full Text PDF:C\:\\Users\\Usuario\\Zotero\\storage\\TTN755VS\\Lema Holguín - 2018 - Implementación de un sistema automático de riego p.pdf:application/pdf},
}


